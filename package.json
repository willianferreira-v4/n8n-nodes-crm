{
	"name": "n8n-nodes-crm",
	"version": "1.0.1",
	"description": "Esse é um pacote de nós personalizado para n8n, focado em integrações com sistemas de CRM. Ele inclui nós para conectar, gerenciar e automatizar fluxos de trabalho relacionados a dados de clientes, facilitando a sincronização e manipulação dessas informações dentro do n8n.",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Willian Silva Ferreira",
		"email": "williansferreir@icloud.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/https://github.com/willianferreira-v4/n8n-nodes-crm.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/CrmApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Example/Example.node.js",
			"dist/nodes/Crm/Crm.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
